@using FribergCarRentals.Mvc.Session
@inject UserSession UserSession

@if (UserSession.IsAdmin())
{
	<partial name="_NavbarAdminPartial" />
}
@if (UserSession.IsCustomer())
{
	<partial name="_NavbarCustomerPartial" />
}
@if (!UserSession.IsAdmin() && !UserSession.IsCustomer())
{
	<partial name="_NavbarPublicPartial" />
}